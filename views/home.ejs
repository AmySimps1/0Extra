/* Cursor */
body {cursor:url('https://cur.cursors-4u.net/nature/nat-10/nat985.cur'), progress!important;}
a, a:hover {cursor:url('https://cur.cursors-4u.net/others/oth-7/oth665.cur'), progress!important;}

/* EDITEDIT */
/* ----- Basic Styling ----- */
 
body {
    
    word-wrap:break-word;
    overflow:auto;
    background-attachment:fixed;
    background-repeat:repeat;
    background-color:#F6F6F6;
    margin:0px;
    color:#000000;
    font:normal 12px/170% 'Arsenal', sans-serif;
    letter-spacing:0.2px;
    -webkit-font-smoothing:antialiased;
    -moz-osx-font-smoothing:grayscale;
}
 
a {
    text-decoration:none;
    border:0; outline:0;
    color:#000000;
    transition:0.3s ease-in-out;
}
 
a:hover {color:#ffe033;}
i, italic, em {color:#000000; text-transform:italic;}
b, bold, strong {color:#000000; font-weight:700;}
img {border:none;}
 
blockquote img {max-width:100%!important; height:auto;}
 
pre, code {
    font:10px consolas, monospace;
    letter-spacing:0px;
    background:#EEEEEE;
    border:1px solid #eaeaea;
    padding:10px;
    display:block;
}

hr {
    border:0px solid transparent;
    color:#000000;
    background-color:#000000;
    width:60%;
    margin:2px 20%;
    height:1px;
}

ul:not(.bxslider) li {list-style-type:none; margin:0; display:block; position:relative;}

ul:not(.bxslider) li::before {
    top:2px;
    font:10px/170% "FontAwesome";
    content:"\f1e3";
    position:absolute;
    left:-20px;
    color:#000000;
}

h1, h2, h3 {
    letter-spacing:1px;
    font:normal 20px 'Tulpen One', sans-serif;
    -webkit-font-smoothing:subpixel-antialiased;
    -moz-osx-font-smoothing:unset;
}

small, sup, sub {
    line-height:150%;
    font-size:10px;
    vertical-align:baseline;
}

big {font-size:12px;}

/* ------ container ------ */

#all {
    margin:0 auto;
    width:calc(540px + 310px);
    padding-left:30px;
}

/* post container */
 
#postmargin {
    position:relative;
    z-index:1;
    padding:50px 50px 0px 50px;
    width:calc(540px + 20px);
}

#postmargin, #postbg {
    background:#EEEEEE;
    border-left:1px solid #eaeaea;
    border-right:1px solid #eaeaea;
    margin-left:184px;
}

#postbg {
    position:fixed;
    top:0px;
    width:calc(540px + 120px);
    height:100%;
    z-index:-1;
}
 
/* ------ posts ------ */
 
.posts {
    background:#F6F6F6;
    padding:0px;
    position:relative;
    border:1px solid #eaeaea;
    width:calc(540px + 20px);
    margin:0px -1px 50px -1px;
}

.container {padding:10px;}
.text {padding:5px 10px; font-weight:400;}
.text a, .sourcez a {border-bottom:1px solid transparent;}
.text a:hover, .sourcez a:hover {border-color:#000000;}
 



blockquote {
    margin:10px;
    display:block;
    padding:0px 10px;
    border-left:1px solid #eaeaea;
}

blockquote.tumblr_parent {
    margin:5px -20px;
    padding:10px 20px;
    background:transparent;
    border-left:none;
    border-top:1px solid #eaeaea;
}

blockquote.tumblr_parent:first-of-type {
    margin:5px 0px 0px 0px;
    padding:5px 0px 0px 0px;
    border-top:none;
}

img.tumblr_avatar + a.tumblr_blog {
    text-transform:uppercase;
    vertical-align:middle;
    display:inline-block;
    letter-spacing:1.5px;
    border-bottom:0px solid transparent;
    font:normal 15px/20px 'Tulpen One', sans-serif;
    -webkit-font-smoothing:subpixel-antialiased;
    -moz-osx-font-smoothing:unset;
}
 
img.tumblr_avatar + a.tumblr_blog:hover {border-bottom:0px solid transparent;}

img.tumblr_avatar {
    width:20px;
    height:20px;
    border-radius:2px;
    margin-right:10px;
    vertical-align:middle;
    display:inline-block;
}


/* photo & video posts */
 
.posts img, .posts iframe {max-width:100%; transition:0.6s ease-in-out;}
 



.photo img {-webkit-filter:grayscale(100%); filter:grayscale(100%);}
.posts:hover .photo img {-webkit-filter:grayscale(0%); filter:grayscale(0%);}

 
.tumblr_video_container {width:auto!important; height:auto!important;}
.photo {padding:10px 10px 0px 10px; margin:-10px -10px 0px -10px;}





.photo a {padding-bottom:0px; border-bottom:0px solid transparent;}

/* text, link, quote posts */
 
.quote {
    padding:5px;
    text-align:center;
    font:400 23px/150% 'Tulpen One', sans-serif;
    letter-spacing:1.5px;
    margin-bottom:15px;
    -webkit-font-smoothing:subpixel-antialiased;
    -moz-osx-font-smoothing:unset;
}
 
.sourcez {
    margin:-15px 0px 5px 0px;
    text-align:center;
    text-transform:uppercase;
    font:400 9px/150% 'Arsenal', sans-serif;
    letter-spacing:0.5px;
}
 
.headt, .llp {
    padding:10px;
    background:#EEEEEE;
    border:1px solid #eaeaea;
    color:#000000;
    text-align:center;
    font:400 20px/150% 'Tulpen One', sans-serif;
    letter-spacing:1.2px;
    -webkit-font-smoothing:subpixel-antialiased;
    -moz-osx-font-smoothing:unset;
}

.jrc, .llp {
    position:relative;
    padding:5px;
    display:block;
    transition:0.6s ease-in-out;
}
 
.llp:hover .jrc {color:#000000;}
.llp:hover {border-color:#000000;}

/* ask posts */
 
.askerp, .askerp:hover {
    width:60px;
    height:60px;
    opacity:1;
    padding:4px;
    display:inline-block;
    border:1px solid #eaeaea;
    background:#EEEEEE;
    position:absolute;
}
 
.tri {
    width:0px;
    height:0px;
    border-style:solid;
    border-width:10px 10px 10px 0;
    border-color:transparent #eaeaea transparent transparent;
    display:inline-block;
    position:absolute;
    margin:15px 0px 0px -25px;
}
 
.tri::before {
    content:"";
    width:0px;
    height:0px;
    border-style:solid;
    border-width:8px 9px 8px 0px;
    border-color:transparent #EEEEEE transparent transparent;
    display:inline-block;
    position:absolute;
    margin:-8px 0px 0px 2px;
}
 
.question {
    padding:9px 14px;
    display:inline-block;
    min-height:50px;
    border:1px solid #eaeaea;
    background:#EEEEEE;
    margin-left:85px;
    width:calc(540px - 115px);
}

/* audio posts */
 
.apbase {height:90px; position:relative;}

.audiocaption {margin-bottom:-10px;}
 
.musicinfo {
    float:left;
    position:relative;
    z-index:2;
    height:70px;
    padding:4px 14px;
    overflow:hidden;
    font:normal 11px/150% 'Arsenal', sans-serif;
    width:calc(540px - 110px);
    background:#EEEEEE;
    border:1px solid #eaeaea;
    display:block;
    transition:0.6s ease-in-out;
}

.musicinfo b {
    display:block;
    text-transform:uppercase;
    font:400 15px/150% 'Tulpen One', sans-serif;
    letter-spacing:1.5px;
    color:#000000;
    -webkit-font-smoothing:subpixel-antialiased;
    -moz-osx-font-smoothing:unset;
}
 
.albumart {
    float:left;
    display:block;
    position:relative;
    z-index:2;
    width:70px;
    height:70px;
    border:5px solid #eaeaea;
    background:url(http://i.imgur.com/tjZqezW.png) no-repeat;
    background-size:70px 70px;
    transition:0.6s ease-in-out;
}
 
.albumart img, .askerp {opacity:1; display:block;}
 
.tplayer {
    width:20px;
    height:30px;
	margin-left:5px;
    overflow:hidden;
    position:relative;
    z-index:1;
    transition:0.6s ease-in-out;
}

.tumblr_audio_player {
    max-width:500px!important;
    margin-left:-5px!important;
    margin-top:-10px!important;
    transform:scale(0.5,0.5);
    transform-origin:0 0;
    -webkit-transform-origin:0 0;
    -webkit-transform:scale(0.5,0.5);
}
   
.playercontainer {
    z-index:1;
	background:#F6F6F6;
    position:absolute;
    opacity:0;
    visibility:hidden;
    margin-left:calc(540px - 200px + 150px);
    border-width:1px 5px 1px 0px;
    border-style:solid;
    border-color:#eaeaea #000000 #eaeaea transparent;
    margin-top:0px;
    padding:24px 5px 24px 0;
    transition:0.6s ease-in-out;
}
 
.posts:hover .musicinfo {width:calc(540px - 145px);}
 
.posts:hover .playercontainer {
    margin-left:calc(540px - 35px);
    visibility:visible;
	opacity:1;
}

/* chat posts */
 
.label {
    margin:0px;
    padding:5px 10px;
    line-height:200%;
    border:1px solid #eaeaea;
    margin-top:5px;
}
 
.label span {font-weight:700; color:#000000;}
.label:nth-of-type(odd) {background:#EEEEEE;}
.label:nth-of-type(odd) span {color:#000000;}
.label:nth-of-type(even) span {color:#ffe033;}
.label:nth-of-type(even) {background:#f3f3f3;}
.label:last-of-type {margin-bottom:10px;}

/* ------ permalinks ------ */

/* index page side permalinks */

.iperma {
    margin-top:10px;
    margin-left:calc(540px + 20px);
    position:absolute;
}

.iperma a.state {
    border:1px solid #eaeaea;
    border-left:0px solid transparent;
    display:block;
    background:#F6F6F6;
    width:36px;
    padding:5px 7px;
    height:30px;
    text-transform:uppercase;
    text-align:right;
    color:#000000;
    font:400 20px/32px 'Tulpen One', sans-serif;
    letter-spacing:1px;
    transition:0.5s ease-in-out;
    -webkit-font-smoothing:subpixel-antialiased;
    -moz-osx-font-smoothing:unset;
}

.iperma a.state:hover {border-color:#ffe033; color:#ffe033;}

a.reblfr {
    margin:14px 14px -9px 14px;
    width:16px;
    height:16px;
    padding:2px;
    display:block;
    background:#F6F6F6;
    border:1px solid #eaeaea;
    transition:0.3s ease-in-out;
}

a.reblfr img {width:16px; height:16px; display:block;}
a.reblfr:hover {border-color:#000000;}

/* index page bottom permalinks */

.sct {
    margin:-50px 0px 50px 0px;
    min-height:30px;
    position:relative;
    z-index:5;
}

a.sic {
    width:16px;
    height:16px;
    padding:7px;
    font-size:15px;
    border:1px solid #eaeaea;
    border-top:0px solid transparent;
    background:#F6F6F6;
    margin:-1px 6px 0px -1px;
    position:relative;
    display:block;
    float:left;
    text-align:center;
    z-index:5;
    transition:0.3s ease-in-out;
}

a.sic i {
    font-size:15px; 
    display:block; 
    font-style:normal;
    color:#000000;
    transition:0.3s ease-in-out;
}

a.sic:hover {border-color:#000000;}
a.sic:hover i {color:#000000;}

.ilike {position:relative;}

.ilike .like_button {
    position:absolute;
    top:0; bottom:0;
    left:0; right:0;
    width:100%;
    height:100%;
    opacity:0;
    z-index:999;
}
 
.like_button iframe {
    width:100%!important;
    height:100%!important;
}

.ilike i {color:#000000;}
.like_button.liked + i {color:;}
.like_button.liked + i.fa-heart:before {content:"\f21e";}

.indextags {
    position:relative;
    display:inline-block;
    width:calc(540px - 100px);
    padding:5px 0px;
    height:20px;
    z-index:999;
    overflow:hidden;
    white-space:nowrap;
}

.indextags a {
    border:1px solid #eaeaea;
    background:#F6F6F6;
    padding:2px 5px;
    text-transform:uppercase;
	font:9px/12px 'Arsenal', sans-serif;
    display:inline-block;
    margin-right:5px;
    color:#000000;
    letter-spacing:0.5px;
    transition:0.3s ease-in-out;
}

.indextags a:hover {border-color:#ffe033; color:#ffe033;}

